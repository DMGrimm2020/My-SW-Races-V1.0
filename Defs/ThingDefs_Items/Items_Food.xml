<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <ThingDef ParentName="MealBase">
        <defName>TGRationPack</defName>
        <label>ration pack</label>
        <description>Ration pack.</description>
        <graphicData>
            <texPath>Things/Item/Meal/TGRationPack</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
        </graphicData>
        <statBases>
            <DeteriorationRate>0.25</DeteriorationRate>
            <MarketValue>5</MarketValue>
            <WorkToMake>450</WorkToMake>
            <Mass>0.1</Mass>
            <Nutrition>0.9</Nutrition>
        </statBases>
        <ingestible>
            <preferability>MealSimple</preferability>
            <optimalityOffsetHumanlikes>-5</optimalityOffsetHumanlikes>
            <optimalityOffsetFeedingAnimals>-10</optimalityOffsetFeedingAnimals>
			<ingestEffect>EatVegetarian</ingestEffect>
            <ingestSound>Meal_Eat</ingestSound>
        </ingestible>
        <comps>
            <li Class="CompProperties_Rottable">
                <daysToRotStart>120</daysToRotStart>
                <rotDestroys>true</rotDestroys>
            </li>
        </comps>
        <tradeability>Buyable</tradeability>
    </ThingDef>

    <ThingDef ParentName="MealBase">
        <defName>TGEnergyCapsule</defName>
        <label>energy capsule</label>
        <description>Energy capsule.</description>
        <graphicData>
            <texPath>Things/Item/Meal/TGEnergyCapsule</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
        </graphicData>
        <statBases>
            <DeteriorationRate>0.25</DeteriorationRate>
            <MarketValue>2</MarketValue>
            <WorkToMake>700</WorkToMake>
            <Mass>0.05</Mass>
            <Nutrition>0.9</Nutrition>
        </statBases>
        <ingestible>
            <preferability>MealSimple</preferability>
            <optimalityOffsetHumanlikes>-5</optimalityOffsetHumanlikes>
            <optimalityOffsetFeedingAnimals>-10</optimalityOffsetFeedingAnimals>
			<ingestEffect>EatVegetarian</ingestEffect>
            <ingestSound>Meal_Eat</ingestSound>
        </ingestible>
        <comps>
            <li Class="CompProperties_Rottable">
                <daysToRotStart>60</daysToRotStart>
                <rotDestroys>true</rotDestroys>
            </li>
        </comps>
        <tradeability>Buyable</tradeability>
    </ThingDef>

</Defs>